{{#each this}}
<div class="container-countries-full">
<div class="flag">
    <article class="card">
        <img src={{flag}} alt={{name}} class="flag__image" />

</div>

<div class="Capital__content">
    <h1>{{name}}</h1>
    <h2 class="card__name">Capital: {{capital}}</h2>
    <h2 class="card__name">Population: {{population}}</h2>
    <h2 class="card__name">Languages:</h2>
    <ul>
        {{#each languages}}
        <li class="languages">{{name}}</li>
        {{/each }}
    </ul>

</div>
</div>


{{/each}}